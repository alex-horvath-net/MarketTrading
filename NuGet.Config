<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
    <!-- Store packages locally inside ./packages (this is fine if you want it) -->
    <add key="globalPackagesFolder" value="packages" />
  </config>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <!-- add internal feed here if needed -->
  </packageSources>
  <disabledPackageSources>
    <add key="Microsoft Visual Studio Offline Packages" value="true" />
  </disabledPackageSources>
  <fallbackPackageFolders>
    <!-- We intentionally leave this section empty to avoid errors in Linux containers -->
  </fallbackPackageFolders>
</configuration>
